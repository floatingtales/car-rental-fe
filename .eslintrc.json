{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/jsx-no-constructed-context-values": 0,
    "react/prop-types": 0,
    "no-console": 0,
    "no-alert": 0
  }
}
